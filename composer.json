        "itil",
        "itsm"      
    ],
    "homepage": "https://github.com/tuankiet1708/core-sla",
    "support": {
        "issues": "https://github.com/tuankiet1708/core-sla/issues",
        "source": "https://github.com/tuankiet1708/core-sla"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Leo Loves Coding",
            "email": "tuankiet1708@gmail.com",
            "homepage": "https://tranletuankiet.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "nesbot/carbon": "^2.25"
    },
    "autoload": {
